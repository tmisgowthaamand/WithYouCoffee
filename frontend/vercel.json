{
  "version": 2,
  "framework": "vite",
  "outputDirectory": "dist",
  "env": {
    "VITE_API_BASE_URL": "https://withyoucoffee.onrender.com"
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1",
      "headers": {
        "cache-control": "public, max-age=604800"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
